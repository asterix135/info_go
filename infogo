                    <TD><SCRIPT language="javascript">
function changeOption(action, actionType, infoType, unitID){
	var href = action + "?" + "actionType=" + actionType;
	if (infoType != ""){
		href = href + "&infoType=" + infoType;
	}
	if (unitID != ""){
		href = href + "&unitID=" + unitID;
	}
	href += "&locale=en"
	document.location.href=href;
}
</SCRIPT>
