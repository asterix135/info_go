<SCRIPT>
function changeHeading(txt) { 
	document.title = txt; 
	var subtitle = document.getElementById("subheading");
	subtitle.innerHTML = "<h1>" + txt + "</h1>";
}	
function changeTitle(txt) {	
	var breadcrumb = document.getElementById("breadleft")
	breadcrumb.innerHTML = "Location: " + txt;
}
function scrollToAnchor(){
	var pos = document.URL.indexOf("position");
	if (pos > 0) {
		var anchorPt = document.URL.substr(pos + 9);
		window.location.href="#" + anchorPt;
	}
}
</SCRIPT>

