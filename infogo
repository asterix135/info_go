<script language="javascript">
function browseEmployee(id){
	var form = document.officeActionForm
	form.action = "employee.do";
	form.id.value = id;
	form.actionType.value = "browse";
	form.submit();
}
//changeHeading("INFO-GO: " + 'Office Telephone');	
</script>

